{
  "name": "discord-bot-backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "migrate": "npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all",
    "prestart": "npm run migrate && npm run seed && npm run lint",
    "start": "nodemon",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "nodemon --build npm start"
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/debug": "^4.1.6",
    "@types/express": "^4.16.1",
    "@types/node": "16.0.0",
    "@types/validator": "13.6.2",
    "tslint": "^5.12.1",
    "typescript": "4.3.5"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "discord.js": "^13.1.0",
    "express": "^4.16.4",
    "express-jwt": "^6.0.0",
    "express-session": "^1.17.2",
    "express-validator": "^6.6.1",
    "mysql2": "^2.2.5",
    "nodemon": "^2.0.9",
    "sanitize": "^2.1.0",
    "sequelize": "^6.6.4",
    "ts-node": "^10.0.0"
  }
}