    "@types/node": "16.0.0",
    "@types/supertest": "^2.0.11",
    "@types/validator": "13.6.2",
    "jest": "^27.2.0",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "tslint": "^5.12.1",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@discordjs/rest": "0.1.0-canary.0",
    "@sapphire/framework": "^1.0.2",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "discord-api-types": "^0.23.1",
    "discord.js": "12.5.3",
    "dotenv": "^10.0.0",
    "express": "^4.16.4",
    "express-jwt": "^6.0.0",
    "express-session": "^1.17.2",
    "express-validator": "^6.6.1",
    "mysql2": "^2.2.5",
    "nodemon": "^2.0.9",
    "sanitize": "^2.1.0",
